{
  "name": "alwz",
  "description": "Extendable library for typecasting.",
  "version": "1.2.0",
  "main": "./build/commonjs/index.js",
  "exports": {
    "require": "./build/commonjs/index.js",
    "import": "./build/es/index.js"
  },
  "scripts": {
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "tsc --project tsconfig.commonjs.json",
    "build:es": "tsc --project tsconfig.es.json",
    "build:development": "tsc --watch",
    "clean": "rm -rf ./build",
    "build:clear": "npm run clean && npm run build",
    "readme": "documentation readme build/es/index.js --section=Usage",
    "preversion": "npm run test",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test:development": "jest --watch",
    "test": "jest"
  },
  "keywords": [
    "casting"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-chepugov/alwz.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-chepugov/alwz/issues"
  },
  "homepage": "https://github.com/a-chepugov/alwz#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-jest": "^27.1.0",
    "documentation": "^13.2.5",
    "eslint": "^7.32.0",
    "jest": "^27.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4"
  }
}
